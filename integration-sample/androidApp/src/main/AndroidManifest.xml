<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ecommpay.msdk.ui.example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App"
        tools:replace="android:theme">
        <activity
            android:name="com.ecommpay.msdk.ui.example.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ecommpay.msdk.ui.example.XmlActivity"
            android:exported="false"
            android:theme="@style/Theme.App"
            android:label="@string/title_activity_xml" />
        <activity
            android:name="com.ecommpay.msdk.ui.example.ComposeActivity"
            android:exported="false"
            android:theme="@style/Theme.App"
            android:label="@string/title_activity_compose" />
    </application>

</manifest>